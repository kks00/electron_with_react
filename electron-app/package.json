    }
  },
  "dependencies": {
    "electron": "^20.0.2",
    "electron-is-dev": "^3.0.1",
    "ffi-napi": "^4.0.3",
    "ref-napi": "^3.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.13.3",
    "wait-on": "^7.2.0"
  }
}
